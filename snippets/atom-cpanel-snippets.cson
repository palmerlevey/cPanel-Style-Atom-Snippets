  "cPanel wrapper":
    "prefix": "wrapper"
    "body": """
      [% css_code_block = PROCESS css_block %]
      [% js_code_block = PROCESS js_block %]
      [% WRAPPER '_assets/master.html.tt'
          app_key = '${1:app_key}'
          page_title = '${2:page_title}'
          page_stylesheets = []
          page_scripts = []
          page_styles = css_code_block
          page_js = js_code_block
          include_legacy_stylesheets = 0 # excludes legacy yui stylesheets
          include_legacy_scripts = 0 # excludes yui utils and x3 optimized scripts
          include_cjt = 0 # excludes legacy cjt scripts
      -%]
      [% END # WRAPPER END %]
      [% BLOCK css_block %]
      <style>
      </style>
      [% END # css_block END %]
      [% BLOCK js_block %]
      <script>
      </script>
      [% END # js_block END %]
    """

