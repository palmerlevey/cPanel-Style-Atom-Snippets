  "cPanel groupbtn":
    "prefix": "groupbtn"
    "body": """
      <div class="btn-group">
        <button id="btn${1:id} type="button"
          class="btn btn-default"
          title="[% locale.maketext('${3:tooltip}') %]">
          [% locale.maketext("${2:Text}") %]
        </button>
        <button id="btn${4:id} type="button"
          class="btn btn-default"
          title="[% locale.maketext('${5:tooltip}') %]">
          [% locale.maketext("${6:Text}") %]
        </button>
      </div>
    """
  "cPanel ibadge":
