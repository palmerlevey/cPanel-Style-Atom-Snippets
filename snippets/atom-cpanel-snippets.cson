  "cPanel jsmt":
    "prefix": "jsmt"
    "body": """
      LOCALE.maketext('{$1:text}')
    """
  "cPanel jsmturl":
    "prefix": "jsmturl"
    "body": """
      LOCALE.maketext('{$1}[output,url,_1,{$2},title,{$3},_2] today!', { class : "{$4}", id : "{$5}", data-{$6} : "{$7}", target : "{$8}"})
    """
  "cPanel lnkbtn":
    "prefix": "lnkbtn"
    "body": """
      <button id="btn${1:ID} type="button"
        class="btn btn-link"
        title="[% locale.maketext('${3:tooltip}') %]">
        [% locale.maketext("${2:Text}") %]
      </button>
    """
  "cPanel mt":
    "prefix": "mt"
    "body": """
      [% locale.maketext("{$1:text}") %]
    """
  "cPanel mturl":
    "prefix": "mturl"
    "body": """
      [% locale.maketext('{$1}[output,url,_1,{$2},title,{$3},_2] today!', { class=>"{$4}", id=>"{$5}", data-{$6}=>"{$7}", target =>"{$8}"}) %]
    """
  "cPanel ngactionbtn":
    "prefix": "ngactionbtn"
    "body": """
      <button id="btn{$1:ID}"
          spinner-id="spinner{$2:ID}"
          action="{$3:Function}">
        {$4: Text}
      </button>
    """
  "cPanel ngalert":
    "prefix": "ngalert"
    "body": """
      <div cp-alert ng-repeat="{$1:alert} in alerts"
            ng-model="{$1:alert}" on-close="{$2:closeAlert}($index)">
          <span ng-bind-html="{$1:alert}.message"></span>
      </div>
    """
  "cPanel ngpg":
    "prefix": "ngpg"
    "body": """
      <pagination id="pager{$1:ID}" direction-links="false" boundary-links="true" total-items="{$2:totalItems}"
        ng-model="{$3:currentPage}" items-per-page="{$4:pageSize}" ng-change="{$5:pageChange}"
        first-text="<<" last-text=">>">
      </pagination>
    """
  "cPanel ngpgellipse":
    "prefix": "ngpgellipse"
    "body": """
      <pagination id="pager{$1:ID}" total-items="{$2:totalItems}" ng-model="{$3:currentPage}"
        max-size="{$4:maxSize}" items-per-page="{$4:pageSize}" ng-change="{$5:pageChange}"
        boundary-links="true" rotate="false" num-pages="{$5:numPages}"
        previous-text="<" next-text=">" first-text="<<" last-text=">>">
      </pagination>
    """
  "cPanel ngpglimited":
    "prefix": "ngpglimited"
    "body": """
      <pagination id="pager{$1:ID}" total-items="{$2:totalItems}" ng-model="{$3:currentPage}" max-size="{$4:maxSize}"
        boundary-links="true" items-per-page="{$4:pageSize}" ng-change="{$5:pageChange}"
        previous-text="<" next-text=">" first-text="<<" last-text=">>">
      </pagination>
    """
