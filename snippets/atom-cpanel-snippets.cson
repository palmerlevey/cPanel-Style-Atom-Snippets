    """
  "cPanel dbtn":
    "prefix": "dbtn"
    "body": """
      <button id="btn${1:Default} type="button"
        class="btn btn-default"
        title="[% locale.maketext('${3:tooltip}') %]">
        [% locale.maketext("${2:Text}") %]
      </button>
    """
